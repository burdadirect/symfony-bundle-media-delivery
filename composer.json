{
  "name": "burdanews/media-delivery-bundle",
  "description": "HBM Media Delivery Bundle - Resize, crop, watermark and generally modify images and deliver them securely.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\MediaDeliveryBundle": ""
    }
  },
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdanews/helper-bundle.git"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-imagick" : "*",
    "symfony/config": "^3.4|^4.2",
    "symfony/dependency-injection": "^3.4|^4.2",
    "symfony/http-foundation": "^3.4|^4.2",
    "symfony/framework-bundle": "^3.4|^4.2",
    "symfony/monolog-bundle" : "^3.1",
    "symfony/twig-bundle": "^3.4|^4.2",
    "doctrine/orm": "^2.5",
    "imagine/imagine" : "dev-master",
    "burdanews/helper-bundle": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "target-dir": "HBM/MediaDeliveryBundle"
}
